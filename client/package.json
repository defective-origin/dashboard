{
  "name": "dashboard-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build:dev": "yarn run verify && vite build --mode development",
    "build:prod": "yarn run verify && vite build --mode production",
    "build:test": "yarn run verify && vite build --mode testing",
    "build:stage": "yarn run verify && vite build --mode staging",
    "build": "yarn run build:prod && yarn run stories:build && cp -r ./storybook-static ./dist/storybook",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "yarn run lint:es && yarn run lint:css",
    "lint:fix": "yarn run lint:es:fix && yarn run lint:css:fix",
    "lint:es": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 100",
    "lint:es:fix": "eslint src --ext .ts,.tsx --report-unused-disable-directives --fix",
    "lint:css": "stylelint \"**/*.scss\" --max-warnings 100",
    "lint:css:fix": "stylelint \"**/*.scss\" --fix",
    "verify": "yarn run lint && yarn run deps && echo FIX TESTS",
    "gen": "plop --plopfile ./generator/generator.parts.js --dest ./src",
    "gen:init": "plop --plopfile ./generator/generator.init.js --dest ./src",
    "stories": "storybook dev -p 6006",
    "stories:build": "storybook build --docs",
    "deps": "dpdm --no-warning --no-tree ./src/main.tsx",
    "deps:tree": "dpdm --no-warning ./src/main.tsx"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/material": "7.0.1",
    "@tanstack/react-query": "5.71.10",
    "axios": "1.8.4",
    "classnames": "2.5.1",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "8.0.4",
    "i18next-http-backend": "3.0.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-helmet": "6.1.0",
    "react-i18next": "15.4.1",
    "react-router-dom": "7.5.0",
    "react-toastify": "11.0.5",
    "recharts": "2.15.2",
    "weak-key": "1.0.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.3.2",
    "@storybook/addon-essentials": "8.0.8",
    "@storybook/addon-interactions": "8.0.8",
    "@storybook/addon-links": "8.0.8",
    "@storybook/addon-onboarding": "8.0.8",
    "@storybook/addon-styling-webpack": "1.0.0",
    "@storybook/addon-themes": "8.0.10",
    "@storybook/blocks": "8.0.8",
    "@storybook/manager-api": "8.0.8",
    "@storybook/react": "8.0.8",
    "@storybook/react-vite": "8.0.8",
    "@storybook/test": "8.0.8",
    "@storybook/theming": "8.0.8",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@types/react-helmet": "6.1.11",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react-swc": "3.6.0",
    "dpdm": "3.14.0",
    "eslint": "8.57.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.6",
    "eslint-plugin-storybook": "0.8.0",
    "jsdom": "24.0.0",
    "plop": "4.0.1",
    "sass": "1.75.0",
    "storybook": "8.0.8",
    "stylelint": "15.11.0",
    "stylelint-config-standard-scss": "10.0.0",
    "stylelint-scss": "5.3.2",
    "typescript": "5.8.3",
    "vite": "6.2.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.1"
  }
}
