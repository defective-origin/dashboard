{
  "name": "dashboard-client",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "yarn lint:es && yarn lint:style",
    "lint:fix": "yarn lint:es:fix && yarn lint:style:fix",
    "lint:es": "eslint . --ext .ts --ext .tsx",
    "lint:es:fix": "eslint . --ext .ts --ext .tsx --fix",
    "lint:style": "stylelint src/**/*{.css,.scss}",
    "lint:style:fix": "stylelint src/**/*{.css,.scss} --fix",
    "prettify": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts)\"",
    "check:changes": "yarn lint && yarn test --watchAll=false && yarn build",
    "gen": "plop"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.5",
    "@reach/router": "^1.3.4",
    "@types/react-helmet": "^6.1.6",
    "classnames": "^2.3.1",
    "i18next": "^23.1.0",
    "i18next-browser-languagedetector": "^7.0.2",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^13.0.0",
    "react-nanny": "^2.15.0",
    "react-resize-detector": "^7.0.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.52.1",
    "weak-key": "^1.0.2",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.0.0",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/resize-observer-browser": "^0.1.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "plop": "^3.1.2",
    "prettier": "^2.8.8",
    "react-scripts": "^5.0.1",
    "stylelint": "^13.8.0",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "typescript": "^5.0.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
