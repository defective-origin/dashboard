{
  "name": "dashboard-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:dev": "tsc && vite build --mode dev",
    "build:prod": "tsc && vite build",
    "build:test": "tsc && vite build --mode testing",
    "build:stage": "tsc && vite build --mode staging",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "yarn lint:es && yarn lint:css",
    "lint:fix": "yarn lint:es:fix && lint:css:fix",
    "lint:es": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:es:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint:css": "stylelint \"**/*.scss\"",
    "lint:css:fix": "stylelint \"**/*.scss\" --fix",
    "check:changes": "yarn lint && yarn test && yarn build",
    "gen": "plop --plopfile ./generator/generator.parts.js --dest ./src",
    "gen:init": "plop --plopfile ./generator/generator.init.js --dest ./src"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.7",
    "classnames": "^2.3.2",
    "i18next": "^23.2.7",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^13.0.1",
    "react-router-dom": "^6.15.0",
    "react-toastify": "^9.1.3",
    "weak-key": "^1.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jsdom": "^22.1.0",
    "plop": "^3.1.2",
    "sass": "^1.63.6",
    "stylelint": "^15.10.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-scss": "^5.0.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.4"
  }
}
